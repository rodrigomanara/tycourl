{
    "name": "codediesel/ticourl",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "ext-zip": "*",
        "swagger-api/swagger-ui": "^5.21",
        "guzzlehttp/guzzle": "^7.9",
        "twig/twig": "^3.0",
        "phpmailer/phpmailer": "*"
    },
    "autoload": {
        "psr-4": {
            "Codediesel\\Controller\\": "Controller/",
            "Codediesel\\Library\\": "Library/",
            "Codediesel\\Model\\": "Model/",
            "Codediesel\\Pages\\": "Pages/",
            "Codediesel\\RestApi\\": "RestApi/",
            "Codediesel\\Exception\\": "Exception/",
            "Codediesel\\Type\\": "Type/"
        }
    },
    "authors": [
        {
            "name": "codediesel",
            "email": "rodrigomanara@yahoo.co.uk"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "symfony/console": "^7.2",
        "web-token/jwt-framework": "@stable",
        "symfony/serializer": "^7.2",
        "symfony/yaml": "^7.2",
        "symfony/dotenv": "^7.2",
        "phpdocumentor/shim": "^3.7",
        "zircote/swagger-php": "^5.1",
        "doctrine/annotations": "^2.0"
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml.dist"
    },
    "config": {
        "allow-plugins": {
            "phpdocumentor/shim": true
        }
    }
}
